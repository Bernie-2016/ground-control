{
  "name": "ground-control",
  "private": true,
  "description": "Every political revolution needs a launch pad.",
  "repository": "Bernie-2016/ground-control",
  "version": "0.1.0",
  "scripts": {
    "bundle": "mustache ./src/frontend/index/production.json ./src/frontend/index/index.mustache > ./src/frontend/public/index.html && mkdir -p ./src/frontend/public/js && webpack --config ./webpack/config.js",
    "postinstall": "NO_DB=true npm run bundle",
    "start": "./run ./src/backend/server.js",
    "predev": "mustache ./src/frontend/index/development.json ./src/frontend/index/index.mustache > ./src/frontend/public/index.html",
    "dev": "nf start -w --procfile Procfile.dev",
    "lint": "eslint src",
    "knex": "knex --knexfile ./src/backend/data/knexfile.js",
    "migrate": "knex migrate:latest --knexfile ./src/backend/data/knexfile.js",
    "seed": "knex seed:run --knexfile ./src/backend/data/knexfile.js"
  },
  "dependencies": {
    "async": "^1.5.0",
    "babel": "^5.8.21",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "babel-register": "^6.2.0",
    "babel-relay-plugin": "^0.3.0",
    "bcrypt": "^0.8.5",
    "bluebird": "^2.10.2",
    "body-parser": "1.14.1",
    "connect-session-knex": "^1.0.17",
    "cron": "^1.1.0",
    "css-loader": "^0.23.0",
    "csv-load-sync": "0.0.3",
    "dataloader": "^1.0.0",
    "dotenv": "^1.2.0",
    "email-templates": "2.0.1",
    "express": "^4.13.1",
    "express-graphql": "^0.3.0",
    "express-history-api-fallback": "2.0.0",
    "express-rate-limit": "^2.0.2",
    "express-session": "^1.4.0",
    "faker": "^3.0.1",
    "fixed-data-table": "^0.6.0",
    "graphql": "^0.4.2",
    "graphql-relay": "^0.3.5",
    "handlebars": "^3.0.0",
    "history": "^1.13.1",
    "humps": "^1.0.0",
    "jquery": "^2.1.4",
    "json-loader": "^0.5.4",
    "json2csv": "^3.0.1",
    "knex": "^0.9.0",
    "mailgun-js": "0.7.6",
    "material-ui": "0.14.0-rc1",
    "minilog": "^3.0.0",
    "moment": "^2.10.6",
    "moment-timezone": "^0.4.1",
    "mustache": "^2.2.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^4.4.3",
    "radium": "^0.14.3",
    "react": "^0.14.2",
    "react-dom": "^0.14.0",
    "react-formal": "^0.14.2",
    "react-google-maps": "^4.7.0",
    "react-kronos": "^1.2.0",
    "react-relay": "^0.4.0",
    "react-router": "^1.0.2",
    "react-router-relay": "^0.7.0",
    "react-select": "^1.0.0-beta8",
    "react-tap-event-plugin": "^0.2.1",
    "request-promise": "^1.0.1",
    "retry": "^0.8.0",
    "stacktrace-js": "^1.0.1",
    "style-loader": "^0.13.0",
    "superagent": "^1.4.0",
    "tz-lookup": "^6.0.1",
    "webpack": "^1.10.5",
